<div class="contextual">
<%= link_to 'Register usage', new_risp_service_usage_path, :class => "icon icon-add"  %> | 
<%= render :partial => 'shared/admin_menu' %>
</div>

<h1>Listing risp_service_usages</h1>

<table class="list">
  <tr>
    <th>Project service</th>
    <th>Created</th>
    <th>Usage</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @risp_service_usages.each do |risp_service_usage| %>
  <tr>
    <td><%= risp_service_usage.risp_project_service.title %></td>
    <td><%= risp_service_usage.created_at %></td>
    <td><%= risp_service_usage.usage %></td>
    <td><%= link_to 'Show', risp_service_usage %></td>
    <td><%= link_to 'Edit', edit_risp_service_usage_path(risp_service_usage) %></td>
    <td><%= link_to 'Destroy', risp_service_usage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

