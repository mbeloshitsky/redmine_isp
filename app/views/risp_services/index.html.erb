<div class="contextual">
<%= link_to 'New service', new_risp_service_path, :class => "icon icon-add" %> | 
<%= render :partial => 'shared/admin_menu' %>
</div>

<h1>Listing risp_services</h1>

<table class="list">
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Munit</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @risp_services.each do |risp_service| %>
  <tr>
    <td><%= risp_service.name %></td>
    <td><%= risp_service.description %></td>
    <td><%= risp_service.munit %></td>
    <td><%= link_to 'Show', risp_service %></td>
    <td><%= link_to 'Edit', edit_risp_service_path(risp_service) %></td>
    <td><%= link_to 'Destroy', risp_service, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

